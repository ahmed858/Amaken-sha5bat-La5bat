            {% set attachments = frappe.get_all("File",
            filters={ "attached_to_name": doc.name },
            fields=["file_url", "is_print_image"]
            )[:6] %}

            {% set filtered_images = [] %}
            {% for attachment in attachments %}
            {% if attachment.is_print_image == 1 %}
            {% set _ = filtered_images.append(attachment) %}
            {% endif %}
            {% endfor %}

            {% for image in filtered_images %}
            {% if loop.index0 % 2 == 0 %}
            <tr>
                {% endif %}

                <td style="text-align: center;">
                    <img src="{{ image.file_url }}"
                        style="width:370px !important; height:240px !important; padding:4px; margin: 2px; border:solid 1px #aaa; border-radius: 6px; image-orientation: auto;" />
                </td>

                {% if loop.index0 % 2 == 1 %}
            </tr>
            {% endif %}
            {% endfor %}

            {% if filtered_images|length % 2 != 0 %}
            </tr> <!-- إغلاق الصف الأخير إذا لم يكن مكتملاً -->
            {% endif %}
        </tbody>
    </table>

</div>
{{ footer }}
<br />

